*code
def censor_vowels_while(word):
    out_str = ""
    i = 0
    while i < len(word):
        char = word[i]
        if char == "a" or char == "e" or char == "i" or char == "o" or char == "u":
            out_str += "*"
        else:
            out_str += char
        i += 1
    return out_str
